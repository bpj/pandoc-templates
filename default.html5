<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$$if(dir)$ dir="$dir$"$endif$>
<head>
$for(code-at.html5.header.start)$
$code-at.html5.header.start$
$endfor$
$for(code-at.html.header.start)$
$code-at.html.header.start$
$endfor$
  <meta charset="utf-8">
  <meta name="generator" content="pandoc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ – $endif$$pagetitle$</title>
  <style type="text/css">
      code{white-space: pre-wrap;}
      span.smallcaps{font-variant: small-caps;}
      div.line-block{white-space: pre-line;}
      div.column{display: inline-block; vertical-align: top; width: 50%;}
$if(quotes)$
      q { quotes: "“" "”" "‘" "’"; }
$endif$
  </style>
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
  <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
  <![endif]-->
$for(code-at.html5.header.end)$
$code-at.html5.header.end$
$endfor$
$for(code-at.html.header.end)$
$code-at.html.header.end$
$endfor$
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
$for(code-at.html5.body.start)$
$code-at.html5.body.start$
$endfor$
$for(code-at.html.body.start)$
$code-at.html.body.start$
$endfor$
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<p class="subtitle">$subtitle$</p>
$endif$
$for(author)$
<p class="author">$author$</p>
$endfor$
$if(date)$
<p class="date">$date$</p>
$endif$
</header>
$endif$
$if(toc)$
$if(toc-title)$
<h1>$toc-title$</h1>
$endif$
<nav id="$idprefix$TOC">
$table-of-contents$
</nav>
$endif$
$body$
$for(code-at.html5.body.end)$
$code-at.html5.body.end$
$endfor$
$for(code-at.html.body.end)$
$code-at.html.body.end$
$endfor$
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
